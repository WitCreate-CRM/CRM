<%- model_class = Preferencia -%>
<div class="page-header">
  <h1>Preferencias</h1>
</div>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>Id</th>
      <th> Tipo De Habitación </th>
      <th> Huésped </th>
      <th> Número De Almohadas En La Cama </th>
      <th></th>
    </tr>
  </thead>
  <tbody>
     <%= render @preferencias %>
  </tbody>
  <tfoot>
    <tr>
      <td><%= link_to 'Nueva Preferencia', new_preferencia_path, :remote => true %></td>
    </tr>
  </tfoot>
</table>