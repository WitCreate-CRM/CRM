<%- model_class = Telefono -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:propietario_id_id) %>:</strong></dt>
  <dd><%= @telefono.propietario_id_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:numero_telefonico) %>:</strong></dt>
  <dd><%= @telefono.numero_telefonico %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              telefonos_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_telefono_path(@telefono), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              telefono_path(@telefono),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
