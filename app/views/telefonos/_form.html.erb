<%= simple_form_for @telefono, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :propietario_id_id %>
  <%= f.input :numero_telefonico %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                telefonos_path, :class => 'btn' %>
  </div>
<% end %>
