<%- model_class = Telefono -%>
<div class="page-header">
  <h1>Telefonos</h1>
</div>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>id</th>
      <th>propietario_id</th>
      <th>numero telefonico</th>
      <th>empleado</th>
      <th>created_at</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
     <%= render @telefonos %>
  </tbody>
  <tfoot>
    <tr>
      <td><%= link_to 'Nuevo Telefono', new_telefono_path, :remote => true %></td>
    </tr>
  </tfoot>
</table>

