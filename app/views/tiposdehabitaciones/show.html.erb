<%- model_class = Tiposdehabitacion -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:descripcion) %>:</strong></dt>
  <dd><%= @tiposdehabitacion.descripcion %></dd>
  <dt><strong><%= model_class.human_attribute_name(:piso) %>:</strong></dt>
  <dd><%= @tiposdehabitacion.piso %></dd>
  <dt><strong><%= model_class.human_attribute_name(:habitacion_tina) %>:</strong></dt>
  <dd><%= @tiposdehabitacion.habitacion_tina %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              tiposdehabitaciones_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_tiposdehabitacion_path(@tiposdehabitacion), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              tiposdehabitacion_path(@tiposdehabitacion),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
