<%- model_class = Tiposdehabitacion -%>
<div class="page-header">
  <h1>Tipos De Habitaciones</h1>
</div>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>Id</th>
      <th>Descripción</th>
      <th>Habitacion Con Tina</th>
      <th>Piso</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
     <%= render @tiposdehabitaciones %>
  </tbody>
  <tfoot>
    <tr>
      <td><%= link_to 'Nuevo Tipo De Habitación', new_tiposdehabitacion_path, :remote => true %></td>
    </tr>
  </tfoot>
</table>
