<%- model_class = Obsequio -%>
<div class="page-header">
  <h1>Obsequios</h1>
</div>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>id</th>
      <th>descripcion</th>
      <th>created_at</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
     <%= render @obsequios %>
  </tbody>
  <tfoot>
    <tr>
      <td><%= link_to 'Nuevo Obsequio', new_obsequio_path, :remote => true %></td>
    </tr>
  </tfoot>
</table>

