<tr id=<%= "contacto_#{contacto.id}" %>>
  
  </td>
  <td><%= contacto.id %></td>
  <td><%= contacto.empresa.nombre %></td>
  <td><%= image_tag contacto.avatar.url(:lsmall) %> </td>
  <td> <%= contacto.nombre%></td>
  <td><%= contacto.celular %></td>
  <td><%= contacto.email %></td>
  <td><%= contacto.fecha_nacimiento %></td>
 
  <td>
    <%= link_to 'Mostrar', contacto, :remote => true, :class => 'btn btn-success btn-mini' %>
    <%= link_to 'Editar', edit_contacto_path(contacto), :remote => true,
        :class => 'btn btn-mini' %>
    <%= link_to 'Eliminar', contacto, 
        :data => { :confirm => 'Esta Seguro?' },
        :method => :delete, :remote => true, :class => 'btn btn-mini btn-danger' %>
  </td>
</tr>
