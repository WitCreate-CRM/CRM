<%- model_class = Contacto-%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<div class='contacto'>
  <p><%= @contacto.id %></p>
  <p><%= @contacto.empresa_id %></p>
  <p><%= @contacto.nombre %></p>
  <p><%= @contacto.celular %></p>
  <p><%= @contacto.email %></p>
  <p><%= @contacto.fecha_nacimiento %></p>
 
 
 
</div>

<td><%= link_to image_tag("pdf.png"), contacto_path(@contacto, :format => "pdf") %></td>