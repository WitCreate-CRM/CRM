<%- model_class = Contacto -%>
<div class="page-header">
  <h1>Contactos</h1>
</div>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>id</th>
      <th>empresa_id</th>
      <th>nombre</th>
      <th>celular</th>
      <th>email</th>
      <th>fecha_nacimiento</th>
      <th>created_at</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
     <%= render @contactos %>
  </tbody>
  <tfoot>
    <tr>
      <td><%= link_to 'Nueva Visita', new_contacto_path, :remote => true %></td>
    </tr>
  </tfoot>
</table>

