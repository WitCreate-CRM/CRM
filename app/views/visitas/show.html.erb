<%- model_class = Visita -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:obsequio_id_id) %>:</strong></dt>
  <dd><%= @visita.obsequio_id_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:empleado_id_id) %>:</strong></dt>
  <dd><%= @visita.empleado_id_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:empresa_id_id) %>:</strong></dt>
  <dd><%= @visita.empresa_id_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:fecha) %>:</strong></dt>
  <dd><%= @visita.fecha %></dd>
  <dt><strong><%= model_class.human_attribute_name(:comentarios) %>:</strong></dt>
  <dd><%= @visita.comentarios %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              visitas_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_visita_path(@visita), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              visita_path(@visita),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
