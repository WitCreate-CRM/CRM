<%- model_class = Visita-%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<div class='visita'>
  <p><%= @visita.id %></p>
  <p><%= @visita.obsequio_id %></p>
  <p><%= @visita.empleado_id %></p>
  <p><%= @visita.empresa_id %></p>
  <p><%= @visita.fecha %></p>
  <p><%= @visita.comentarios %></p>
 
 
</div>

<td><%= link_to image_tag("pdf.png"), visita_path(@visita, :format => "pdf") %></td>