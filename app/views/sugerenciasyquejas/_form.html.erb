<%= simple_form_for @sugerenciasyqueja, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :huesped_id %>
  <%= f.input :observaciones %>
  <%= f.input :sugerencias %>
  <%= f.input :problemas_en_la_estadia %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                sugerenciasyquejas_path, :class => 'btn' %>
  </div>
<% end %>
