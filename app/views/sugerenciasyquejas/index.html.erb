<%- model_class = Sugerenciasyqueja -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:huesped_id_id) %></th>
      <th><%= model_class.human_attribute_name(:observaciones) %></th>
      <th><%= model_class.human_attribute_name(:sugerencias) %></th>
      <th><%= model_class.human_attribute_name(:problemas_en_la_estadia) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @sugerenciasyquejas.each do |sugerenciasyqueja| %>
      <tr>
        <td><%= link_to sugerenciasyqueja.id, sugerenciasyqueja_path(sugerenciasyqueja) %></td>
        <td><%= sugerenciasyqueja.huesped_id_id %></td>
        <td><%= sugerenciasyqueja.observaciones %></td>
        <td><%= sugerenciasyqueja.sugerencias %></td>
        <td><%= sugerenciasyqueja.problemas_en_la_estadia %></td>
        <td><%=l sugerenciasyqueja.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_sugerenciasyqueja_path(sugerenciasyqueja), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      sugerenciasyqueja_path(sugerenciasyqueja),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_sugerenciasyqueja_path,
            :class => 'btn btn-primary' %>
